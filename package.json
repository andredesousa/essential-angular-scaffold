{
  "name": "angular11",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "ng serve --open",
    "start:e2e": "ng serve --prod",
    "check": "npm run lint && npm run test",
    "test": "ng test --no-progress --code-coverage",
    "test:watch": "ng test --no-progress --code-coverage --watch",
    "test:debug": "ng test --no-progress --code-coverage --watch --browsers=ChromeDebugging",
    "lint": "npm-run-all -s -c lint:ts lint:scss prettier",
    "lint:fix": "npm-run-all -s -c lint:ts:fix lint:scss:fix prettier:fix",
    "lint:ts": "ng lint",
    "lint:ts:fix": "ng lint --fix",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:scss:fix": "stylelint \"src/**/*.scss\" --fix",
    "prettier": "prettier --check \"src/**/*.{scss,html}\"",
    "prettier:fix": "prettier --write \"src/**/*.{scss,html}\"",
    "docs": "compodoc -p tsconfig.json src -d dist/documentation",
    "audit": "npm audit --production --json | npm run audit:html && npm run audit:check",
    "audit:html": "npm-audit-html --output dist/audit/index.html",
    "audit:check": "npm audit --production --audit-level critical",
    "clean": "rimraf dist/ .lighthouseci/",
    "webdriver-manager:update": "webdriver-manager update --standalone false --gecko false",
    "e2e": "rimraf dist/e2e && ng e2e --webdriverUpdate=false",
    "e2e:watch": "rimraf dist/e2e && npm-run-all -p start:e2e protractor:watch",
    "protractor": "protractor e2e/protractor.conf.override.js",
    "protractor:watch": "wait-on http://localhost:4200/ && watch \"npm run protractor\" e2e/src --wait=1",
    "protractor:debug": "node --inspect-brk node_modules/protractor/bin/protractor e2e/protractor.conf.override.js",
    "build": "ng build --prod",
    "postbuild": "gzipper compress dist/angular11",
    "prerelease": "npm version major && git push",
    "release": "docker build -f ci/docker/Dockerfile -t angular11 .",
    "deploy": "ansible-playbook ci/deploy/deploy-to-swarm.yaml",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lighthouse": "lhci autorun --config=ci/lighthouse/lighthouserc.json",
    "postci": "npm run postinstall",
    "postinstall": "npm-run-all -s webdriver-manager:update ivy",
    "ivy": "ngcc"
  },
  "dependencies": {
    "@angular/animations": "11.1.1",
    "@angular/common": "11.1.1",
    "@angular/compiler": "11.1.1",
    "@angular/core": "11.1.1",
    "@angular/forms": "11.1.1",
    "@angular/platform-browser": "11.1.1",
    "@angular/platform-browser-dynamic": "11.1.1",
    "@angular/router": "11.1.1",
    "rxjs": "6.6.3",
    "tslib": "2.1.0",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.2",
    "@angular/cli": "^11.1.2",
    "@angular/compiler-cli": "~11.1.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@compodoc/compodoc": "^1.1.11",
    "@lhci/cli": "^0.6.1",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.1",
    "conventional-changelog-cli": "^2.1.0",
    "gzipper": "^4.4.0",
    "husky": "^4.2.5",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.2",
    "npm-audit-html": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "protractor-beautiful-reporter": "^1.3.6",
    "rimraf": "^3.0.2",
    "stylelint": "^13.9.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-eslint-rules-recommended": "^1.2.2",
    "tslint-jasmine-rules": "^1.6.1",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~4.0.2",
    "wait-on": "^5.2.1",
    "watch": "^1.0.2",
    "webdriver-manager": "^12.1.8"
  },
  "engines": {
    "node": ">=12.13.0",
    "npm": ">=6.12.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  }
}
